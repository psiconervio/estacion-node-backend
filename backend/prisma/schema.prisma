// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema
generator client {
  provider = "prisma-client-js"
}

// BASEDEDATOSPOSTGRESS
datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_PRISMA_URL")
}

model Station {
  id              Int              @id @default(autoincrement())
  name            String
  latitude        Float?
  longitude       Float?
  description     String?
  createdAt       DateTime         @default(now())
  // Relación inversa a WeatherRecord
  weatherRecords  WeatherRecord[]
  // Relación inversa a DailyAverage, WeeklyAverage, MonthlyAverage
  dailyAverages   DailyAverage[]
  weeklyAverages  WeeklyAverage[]
  monthlyAverages MonthlyAverage[]
}

model WeatherRecord {
  id          Int      @id @default(autoincrement())
  stationId   Int
  station     Station  @relation(fields: [stationId], references: [id])
  temperature Float
  humidity    Float
  anemometro  Float?
  veleta      String
  pluviometro Float
  recordedAt  DateTime @default(now())
}

model DailyAverage {
  id                Int      @id @default(autoincrement())
  stationId         Int
  station           Station  @relation(fields: [stationId], references: [id])
  date              DateTime
  averageTemp       Float
  averageHumidity   Float
  averageAnemometro Float
  averageVeleta     String
  averagePluviomero Float
  createdAt         DateTime @default(now())
}

model WeeklyAverage {
  id                Int      @id @default(autoincrement())
  stationId         Int
  station           Station  @relation(fields: [stationId], references: [id])
  year              Int
  week              Int
  averageTemp       Float
  averageHumidity   Float
  averageAnemometro Float
  averageVeleta     String
  averagePluviomero Float
  createdAt         DateTime @default(now())
}

model MonthlyAverage {
  id                Int      @id @default(autoincrement())
  stationId         Int
  station           Station  @relation(fields: [stationId], references: [id])
  year              Int
  month             Int
  averageTemp       Float
  averageHumidity   Float
  averageAnemometro Float
  averageVeleta     String
  averagePluviomero Float
  createdAt         DateTime @default(now())
}

